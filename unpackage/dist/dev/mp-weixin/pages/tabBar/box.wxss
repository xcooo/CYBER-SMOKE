.biankuang {
  position: absolute;
  left: 40rpx;
  top: 10rpx;
  width: 328rpx;
  height: 90rpx;
  border-radius: 20rpx;
  -webkit-transform: perspective(200rpx) rotateX(10deg);
          transform: perspective(200rpx) rotateX(10deg);
  overflow: hidden;
  box-shadow: 0 0 10rpx #c82fff;
  z-index: 1;
}
.biankuang>text {
  position: absolute;
}
.biankuang>text:nth-child(1) {
  width: 100%;
  height: 3px;
  background: -webkit-linear-gradient(left, transparent, #b42fff);
  left: -100%;
  top: 0px;
  -webkit-animation: line1 2s linear infinite;
          animation: line1 2s linear infinite;
}
@-webkit-keyframes line1 {
50%,
  100% {
    left: 100%;
}
}
@keyframes line1 {
50%,
  100% {
    left: 100%;
}
}
.biankuang>text:nth-child(2) {
  width: 3px;
  height: 100%;
  background: -webkit-linear-gradient(top, transparent, #b42fff);
  right: 0px;
  top: -100%;
  -webkit-animation: line2 2s 0.25s linear infinite;
          animation: line2 2s 0.25s linear infinite;
}
@-webkit-keyframes line2 {
50%,
  100% {
    top: 100%;
}
}
@keyframes line2 {
50%,
  100% {
    top: 100%;
}
}
.biankuang>text:nth-child(3) {
  width: 100%;
  height: 3px;
  background: -webkit-linear-gradient(left, #b42fff, transparent);
  left: 100%;
  bottom: 0px;
  -webkit-animation: line3 2s 0.75s linear infinite;
          animation: line3 2s 0.75s linear infinite;
}
@-webkit-keyframes line3 {
50%,
  100% {
    left: -100%;
}
}
@keyframes line3 {
50%,
  100% {
    left: -100%;
}
}
.biankuang>text:nth-child(4) {
  width: 3px;
  height: 100%;
  background: -webkit-linear-gradient(top, transparent, #b42fff);
  left: 0px;
  top: 100%;
  -webkit-animation: line4 2s 1s linear infinite;
          animation: line4 2s 1s linear infinite;
}
@-webkit-keyframes line4 {
50%,
  100% {
    top: -100%;
}
}
@keyframes line4 {
50%,
  100% {
    top: -100%;
}
}
@-webkit-keyframes guang {
50%,
  100% {
    -webkit-transform: translateX(370rpx) skewX(30deg);
            transform: translateX(370rpx) skewX(30deg);
}
}
@keyframes guang {
50%,
  100% {
    -webkit-transform: translateX(370rpx) skewX(30deg);
            transform: translateX(370rpx) skewX(30deg);
}
}
.biankuang:after {
  content: '';
  position: absolute;
  width: 80rpx;
  height: 100%;
  z-index: 1;
  background-color: rgba(200, 47, 255, .5);
  -webkit-transform: translateX(-100rpx) skewX(30deg);
          transform: translateX(-100rpx) skewX(30deg);
  transition: 0.75s ease-in-out;
  overflow: hidden;
  -webkit-animation: guang 2.5s 1s linear infinite;
          animation: guang 2.5s 1s linear infinite;
  box-shadow: 0 0 10rpx #c82fff;
}

