.page-wrap {
  background: url("https://img.alicdn.com/imgextra/i4/2200676927379/O1CN01unkdtT24NdWplMdw8_!!2200676927379.png") no-repeat;
  background-size: 100vw 100%;
  min-height: calc(100vh - 50px);
  padding-bottom: 40rpx;
}
.page-wrap .search-box {
  background: #fff;
  display: flex;
  align-items: center;
  height: 88rpx;
  background: #222333;
}
.page-wrap .search-box .back {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 10rpx 10rpx 20rpx;
}
.page-wrap .search-box-content {
  width: 450rpx;
}
.page-wrap .search-box  .uni-searchbar__cancel {
  color: #fff;
}
.page-wrap .search-box  .uni-searchbar {
  padding: 10rpx;
  display: flex;
  align-items: center;
}
.page-wrap .search-box  .uni-searchbar .uni-searchbar__box {
  padding: 0 10rpx 0 0 !important;
  height: 60rpx !important;
  display: flex !important;
  align-items: center !important;
}
.page-wrap .search-box  .uni-searchbar .uni-searchbar__box-icon-clear {
  line-height: 60rpx !important;
}
.page-wrap .title-del {
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #999;
}
.page-wrap .title-del .icon {
  width: 33rpx;
  height: 33rpx;
}
.page-wrap .search-list {
  padding: 10rpx 30rpx;
  display: flex;
  flex-flow: row wrap;
}
.page-wrap .search-list-item {
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  background: #fff;
  color: #666;
  margin-top: 20rpx;
  margin-right: 30rpx;
}
